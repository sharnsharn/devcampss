<h1>Portfolio Items</h1>

<!--<a href="portfolios/new" title="">Create New Item</a>-->

<%= link_to "Create New Item", new_portfolio_path%>

<% @portfolios.each do |portfolio_item|%>
<p><%= portfolio_item.title %></p>
<p><%= portfolio_item.subtitle %></p>
<p><%= portfolio_item.body %></p>
<%= image_tag portfolio_item.thumb_image unless portfolio_item.thumb_image.nil? %>
<%= link_to "Edit #{portfolio_item.id}", edit_portfolio_path(portfolio_item.id)%>

<a href="portfolios/<%=portfolio_item.id%>/edit" title="">Edit two<%=portfolio_item.id%></a>
<% end %>